<div class="favorites_panel">
  <div {{= click(this.toggle, this) }} class="favorite-panel--handler"></div>
  <div class="favorite_panel__container">
    <h3>Favorites</h3>
    {{? this.favoritePois.length === 0 }}
      {{= _('favorite folder is empty') }}
    {{?}}
    <ul>
      {{~ this.favoritePois:poi }}
        <li class="favorite_panel__item">
          <p>
            {{= poi.title ? poi.title : 'default' }}
          </p>
          <button class="favorite_panel__remove" {{= click(this.del, this, poi) }}>×</button>
          <button class="favorite_panel__go" {{= click(this.go, this, poi) }}>▷</button>
        </li>
      {{~}}
    </ul>
    <button onclick="fire('store_clear')">Clear</button>
  </div>
</div>
