<h4 class="poi_panel__info__section__title">{{= this.name }}</h4>
<div {{= click(this.extend, this) }}>{{= this.status() }}</div>
<div class="poi_panel__info__hours">
  <table>
    {{~ Object.keys(this.hours):day }}
      <tr>
        <td class="day">{{= day }}</td>
        <td class="hours"> {{? this.hours[day] }} {{= (this.hours[day] + '').replace(',', ' - ') }} {{??}} - no - {{?}}<td>
      </tr>
    {{~}}
  </table>
</div>
