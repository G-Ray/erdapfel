<h4 class="poi_panel__info__section__title poi_panel__section__symbol poi_panel__section__symbol--time">{{= this.name }}</h4>
<div {{= click(this.extend, this) }}>
  <span class="poi_panel__info__hours__status" style="color:#{{= this.status.color }}">{{= this.status.msg }}</span>
</div>
<div class="poi_panel__info__hours">
  <table>
    {{~ Object.keys(this.hours):day }}
      <tr>
        <td class="day">{{= day }}</td>
        <td class="hours"> {{? this.hours[day] }} {{= (this.hours[day] + '').replace(',', ' - ') }} {{??}} - no - {{?}}<td>
      </tr>
    {{~}}
  </table>
</div>
