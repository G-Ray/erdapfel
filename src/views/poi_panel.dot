<div class="poi_panel {{= this.active ? '' : 'poi_panel--hidden' }}">
  <div class="poi_panel__content">
  {{? this.poi }}
    <div class="poi_panel__header_image"></div>
    <div class="poi_panel__logo_demo"></div>
    <div class="poi_panel__description_container">
      <h4 class="poi_panel__title poi_panel__title__symbol poi_panel__title__symbol--park">{{= this.poi.title }}</h4>
      {{? this.poi.description }}
        <p class="poi_panel__description">{{= this.poi.description.replace(/_/g, ' ') }}</p>
      {{?}}
      <div class="poi_panel__store_status">
        <button class="poi_panel__store_status__toggle {{= this.poi.stored ? 'poi_panel__store_status__toggle--stored icon-star-full' : 'icon-star-empty'}}" {{= click(this.toggleStorePoi, this) }}>
        </button>
      </div>

    </div>

    {{= this.poiBlocContainer.set(this.poi).render() }}

  {{?}}
  <br>
  </div>
  {{? this.poi }}
    <div class="poi_panel__close" {{= click(this.toggle, this) }}>ðŸž¨</div>
  {{?}}
</div>

